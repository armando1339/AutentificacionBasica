<h1>Sign up</h1>


<div>

	<%= form_for(@user) do |f| %>

		<%= render 'shared/user_error_messages' %>

		<%= f.label :name %>
		<%= f.text_field :name %>

		<%= f.label :email %>
		<%= f.email_field :email %>

		<%= f.label :password %>
		<%= f.password_field :password %>

		<%= f.label :password_confirmation, "Confirmar password" %>
		<%= f.password_field :password_confirmation %>

		<%= f.submit "Crear cuenta" %>
		
	<% end %>

</div>


